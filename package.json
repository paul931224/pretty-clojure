{
	"name": "pretty-clojure",
	"description": "A clojure code formatter, built on parinfer.js ",
	"version": "0.0.3",
	"publisher": "MartinPaulCristian",
	"repository": "https://github.com/Microsoft/vscode-extension-samples/helloworld-minimal-sample",
	"engines": {
		"vscode": "^1.25.0"
	},
	"activationEvents": [
		"onCommand:extension.parinfer",
		"onLanguage:clojure"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.parinfer",
				"title": "Parinfer"
			}
		]
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install"
	},
	"devDependencies": {
		"shadow-cljs": "^2.20.14",
		"vscode": "^1.1.22"
	},
	"dependencies": {
		"@vscode/vsce": "^2.16.0",
		"parinfer": "^3.13.1"
	}
}